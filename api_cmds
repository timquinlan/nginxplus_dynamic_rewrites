curl -s -X PATCH -d '{"/abc":null}' localhost/api/8/http/keyvals/timebounds
curl -s -X PATCH -d '{"/def":null}' localhost/api/8/http/keyvals/rewrites
curl -s -X PATCH -d '{"/def":null}' localhost/api/8/http/keyvals/timebounds


